{
  "name": "dandelion/dandelion",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "keywords": [
    "Monorepo"
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-phar": "*",
    "monolog/monolog": "~2.0.2",
    "pimple/pimple": "~3.3.0",
    "phpdocumentor/reflection-docblock": "~5.1.0",
    "symfony/console": "~5.0.4",
    "symfony/finder": "~5.0.4",
    "symfony/process": "~5.0.4",
    "symfony/property-access": "~5.0.4",
    "symfony/property-info": "~5.0.4",
    "symfony/serializer": "~5.0.4",
    "swaggest/json-schema": "~0.12.28"
  },
  "require-dev": {
    "codeception/codeception": "^4.1",
    "mikey179/vfsstream": "^1.6",
    "php-coveralls/php-coveralls": "^2.2",
    "phpmd/phpmd": "^2.8",
    "phpro/grumphp-shim": "^0.18",
    "phpstan/phpstan": "^0.12",
    "roave/security-advisories": "dev-master as 1.0.0",
    "sebastian/phpcpd": "^5.0",
    "sllh/composer-versions-check": "^2.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Dandelion\\": "src/Dandelion"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dandelion\\": "tests/Dandelion"
    }
  },
  "bin": "bin/dandelion",
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "github-protocols": [
      "https"
    ]
  }
}
